{
  "editor.defaultFormatter": "esbenp.prettier-vscode",

  "liveSassCompile.settings.formats": [
    {
      "format": "expanded",
      "extensionName": ".css",
      "savePath": "/css"
    },
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/css"
    }
  ],
  "liveSassCompile.settings.exclude": ["**/node_modules/**", ".vscode/**"],
  "liveServer.settings.port": 1237,
  "liveSassCompile.settings.autoprefix": null
}
